
@model FinalProject.Models.Contacts





@{
    ViewBag.Title = "Contact/ContactDisplay";



}

<div id="test">

    <h2>
        @Session["name"]

</h2>




<table border="1" style="background-color:#FFFFCC;border-collapse:collapse;border:1px solid #FFCC00;color:#000000;width:100%">



  
    <h1> your are editing the folllowing <br />
        @Session["Username"]<br />
        @Session["CustomersFriendNumber"]
    
    </h1>
        <tr>
           
            <td width="25%">
                <div id="Wrapper">

                </div><div class="editor -label">
                    @Html.LabelFor(model => model.FirstName)
                </div>

                <div class="editor -label">
                    @Html.ValidationMessageFor(model => model.FirstName)
                    @Html.TextBoxFor(model => model.FirstName, new { @class = "form-control", placeholder = "Persons Name here", tabindex = 1 })
                </div>

                <div class="editor -label">
                    @Html.ValidationMessageFor(model => model.CustomersFriendNumber)
                    @Html.TextBoxFor(model => model.CustomersFriendNumber, new { @class = "form-control", placeholder = "Persons Mobile Number here", tabindex = 1 })
                </div>
                <div class="form-group">
                    
                        
                            <button class="btn btn-primary" name="submit" type="submit">
                                
                                    Submit
                            </button>
                    </div>
            </td>


        </tr>





    
</table>

</div>


@Scripts.Render("~/bundles/jquery")
<script src="~/Models/Stylejs.js"></script>



